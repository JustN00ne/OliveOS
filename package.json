{
  "name": "oliveos",
  "version": "0.1.0",
  "description": "OliveOS :)",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon",
    "close-port": "for /f \"tokens=5\" %a in ('netstat -aon ^| findstr :3000 ^| findstr LISTENING') do taskkill /F /PID %a",
    "tdev": "nodemon --watch src --ext ts --exec ts-node ./server.ts"
  },
  "keywords": [
    "os",
    "olive",
    "webos"
  ],
  "author": "JustNoone",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "adm-zip": "^0.5.10",
    "cheerio": "^1.1.0",
    "commander": "^11.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-handlebars": "^6.0.7",
    "fs-extra": "^11.1.1",
    "iconv-lite": "^0.6.3",
    "lucide": "^0.515.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.1",
    "@types/node-fetch": "^2.6.12",
    "@types/request": "^2.48.12",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
